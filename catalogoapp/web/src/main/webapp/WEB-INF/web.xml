<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	      version="3.0">
  <display-name>Archetype Created Web Application</display-name>
 
  <servlet>
  	<servlet-name>LoginCatalogoAppServlet</servlet-name>
  	
  	<servlet-class>com.ipartek.formacion.catalogoapp.web.LoginCatalogoAppServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AltaCatalogoAppServlet</servlet-name>
  	
  	<servlet-class>com.ipartek.formacion.catalogoapp.web.AltaCatalogoAppServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UsuarioCatalogoAppCrudServlet</servlet-name>
  	
  	<servlet-class>com.ipartek.formacion.catalogoapp.web.UsuarioCatalogoAppCrudServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TiendaCatalogoAppCrudServlet</servlet-name>
  	
  	<servlet-class>com.ipartek.formacion.catalogoapp.web.TiendaCatalogoAppCrudServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UsuarioCatalogoAppFormServlet</servlet-name>
  	
  	<servlet-class>com.ipartek.formacion.catalogoapp.web.UsuarioCatalogoAppFormServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TiendaCatalogoAppFormServlet</servlet-name>
  	
  	<servlet-class>com.ipartek.formacion.catalogoapp.web.TiendaCatalogoAppFormServlet</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	
 

  <servlet-mapping>
  	<servlet-name>LoginCatalogoAppServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AltaCatalogoAppServlet</servlet-name>
  	<url-pattern>/alta</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UsuarioCatalogoAppCrudServlet</servlet-name>
  	<url-pattern>/usuario</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TiendaCatalogoAppCrudServlet</servlet-name>
  	<url-pattern>/tienda</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UsuarioCatalogoAppFormServlet</servlet-name>
  	<url-pattern>/usuarioform</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TiendaCatalogoAppFormServlet</servlet-name>
  	<url-pattern>/tiendaform</url-pattern>
  </servlet-mapping>

</web-app>
